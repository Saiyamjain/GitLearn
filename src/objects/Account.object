    <validationRules>
        <fullName>VAT_Id_format_check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  
	NOT( ISBLANK( Vat_Id__c ) ), 
	NOT( REGEX( Vat_Id__c ,&quot;^(?i)(ATU[0-9]{8}|BE[0-9]{10}|BG[0-9]{9,10}|CY[0-9]{8}[A-Z]|CZ[0-9]{8,10}|DE[0-9]{9}|DK[0-9]{8}|EE[0-9]{9}|EL[0-9]{9}|ES[0-9A-Z][0-9]{7}[0-9A-Z]|FI[0-9]{8}|FR[0-9A-Z]{2}[0-9]{9}|GB([0-9]{9}([0-9]{3})?|[A-Z]{2}[0-9]{3})|HU[0-9]{8}|HR[0-9]{11}|IE[0-9]{7}[A-Z]{1,2}|IE[0-9][A-Z][0-9]{5}[A-Z]|IT[0-9]{11}|LT([0-9]{9}|[0-9]{12})|LU[0-9]{8}|LV[0-9]{11}|MT[0-9]{8}|NL[0-9]{9}B[0-9]{2}|NO[0-9]{9}|PL[0-9]{10}|PT[0-9]{9}|RO[0-9]{2,10}|RS[0-9]{9}|SE[0-9]{12}|SI[0-9]{8}|SK[0-9]{10}|XI([A-Z]{2}[0-9]{3}|[0-9]{9,12}))$&quot;))
)</errorConditionFormula>
        <errorMessage>VAT Id should be in the format DE123456789 or ATU12345678</errorMessage>
    </validationRules>
